<% fb = SharingTags.config.current_context[:facebook] %>

<% if fb && app_id = fb.attributes[:app_id]  %>
try
  window.fbAsyncInit = ->
    window.FB.init(
      appId: '<%= app_id %>',
      status : false,
      # or v2.0, v2.1, v2.0
      version: 'v2.3'
    )

  @SharingTags.FacebookShare.init()
catch error
  console?.error("SharingTags.FacebookShare.init error: #{error}")

<% end  %>
